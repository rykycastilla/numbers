{
  "expo": {
    "name": "Numbers",
    "slug": "Numbers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/main.png",
    "userInterfaceStyle": "auto",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3F3F3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rykyce.numbers"
    },
    "extra": {
      "eas": {
        "projectId": "fb40292c-46e3-4fa8-ae06-dcbff3e4e496"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
